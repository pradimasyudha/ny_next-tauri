{
  "name": "ny-client",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:android": "bun tauri android build --apk",
    "build:desktop": "bun tauri build",
    "build:ios": "bun tauri ios build --export-method app-store-connect",
    "build:mac": "bun tauri build --bundles dmg",
    "build:win-amd64": "bun tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc",
    "build:win-arm64": "bun tauri build --runner cargo-xwin --target aarch64-pc-windows-msvc",
    "clean": "bash -c 'rm -rf ./.next ./build ./out ./src-tauri/{gen,scripts,target}'",
    "dev": "next dev --turbopack",
    "lint": "next lint",
    "out": "mkdir -p ./out && find ./src-tauri -type f \\( -iname \"*.apk\" -o -iname \"*-setup.exe\" -o -iname \"*.deb\" -o -iname \"*.rpm\" -o -iname \"*.ipa\" -o -iname \"*.dmg\" \\) -exec mv {} ./out/ \\;",
    "setup": "bun clean && bun i && bun setup:android && bun setup:desktop && bun setup:ios",
    "setup:android": "bun tauri android init",
    "setup:desktop": "mkdir -p ./src-tauri/scripts && bash -c 'touch ./src-tauri/scripts/{postinstall.sh,preinstall.sh,preremove.sh,postremove.sh}'",
    "setup:ios": "bun tauri ios init",
    "start": "next start",
    "start:desktop": "bun tauri dev",
    "start:android": "bun tauri android dev",
    "start:ios": "bun tauri ios dev",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "15.0.2",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-icons": "^5.3.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
